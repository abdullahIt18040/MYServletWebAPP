<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>myservlet</servlet-name>
    <servlet-class>AddStudent</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>myservlet</servlet-name>
    <url-pattern>/myservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>allstudent</servlet-name>
    <servlet-class>AllStudent</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>allstudent</servlet-name>
    <url-pattern>/allstudent</url-pattern>
  </servlet-mapping>
  
  
  <context-param>
    <param-name>jdbcURl</param-name>
    <param-value>jdbc:mysql://localhost:3306/bookstore</param-value>
  </context-param>
  
  <context-param>
    <param-name>jdbcUsername</param-name>
    <param-value>root</param-value>
  </context-param>
  
  <context-param>
    <param-name>jdbcpassword</param-name>
    <param-value></param-value>
  </context-param>


<welcome-file-list>
  <welcome-file>addstudent.jsp</welcome-file>
</welcome-file-list>



</web-app>
